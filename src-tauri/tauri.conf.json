{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "occult",
  "version": "0.1.0",
  "identifier": "io.github.occult-chat.occult",
  "mainBinaryName": "occult",
  "build": {
    "beforeBuildCommand": "npm run next-build",
    "beforeDevCommand": "npm run next-start",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "title": "Occult",
        "width": 1280,
        "height": 800,
        "transparent": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "",
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  }
}